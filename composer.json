{
    "name": "sunxyw/unify-shorten",
    "description": "This is my package unify-shorten",
    "keywords": [
        "sunxyw",
        "laravel",
        "unify-shorten"
    ],
    "homepage": "https://github.com/sunxyw/unify-shorten",
    "license": "MIT",
    "authors": [
        {
            "name": "sunxyw",
            "email": "dev@sunxyw.xyz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-package-tools": "^1.13.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Sunxyw\\Shorten\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sunxyw\\Shorten\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sunxyw\\Shorten\\ShortenServiceProvider"
            ],
            "aliases": {
                "Shorten": "Sunxyw\\Shorten\\Facades\\Shorten"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
